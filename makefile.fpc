[package]
name=ekron_realms
version=1.0

[compiler]
# Enable object-oriented mode and debugging
options=-Mobjfpc -Scgi -gl -Xs -O2 -vewn

# Target platform
target=darwin

# Main program entry
main=quake2d.lpr

[install]
# Output binary location
bindir=bin
libdir=lib

# Files to install
files=my_project:$(bindir)/ekron_realms

